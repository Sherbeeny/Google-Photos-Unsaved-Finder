{
  "name": "google-photos-saved-finder",
  "version": "2025.08.29-0440",
  "description": "A userscript that works with the Google Photos Toolkit (GPTK) to find photos in a specific album based on their 'saved' status and add them to another album for easy bulk actions.",
  "scripts": {
    "build": "./node_modules/.bin/rollup -c",
    "lint": "eslint src/**/*.js",
    "prepare": "node tools/fetch-userscripts.js && node tools/patch-gptk.js",
    "patch-gptk": "node tools/patch-gptk.js",
    "pretest": "npm run build",
    "test": "playwright test tests/e2e.test.js",
    "validate": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sherbeeny/Google-Photos-Saved-Finder.git"
  },
  "keywords": [
    "userscript",
    "google-photos",
    "tampermonkey"
  ],
  "author": "Jules & Sherbeeny",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Sherbeeny/Google-Photos-Saved-Finder/issues"
  },
  "homepage": "https://github.com/Sherbeeny/Google-Photos-Saved-Finder#readme",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/browser": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "jsdom": "^26.1.0",
    "rollup": "^4.48.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-string": "^3.0.0",
    "selfsigned": "^3.0.1",
    "vite-plugin-string": "^1.2.3",
    "vitest": "^3.2.4"
  }
}
